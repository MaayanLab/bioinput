{
  "name": "binput",
  "version": "1.0.0",
  "description": "An autocomplete input element for biological terms.",
  "main": "bioinput.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "sass": "sass src/bioinput.scss:dist/bioinput.css",
    "sass:watch": "sass src/bioinput.scss:dist/bioinput.css --watch",
    "sass:min": "sass src/bioinput.scss:dist/bioinput.min.css --style compressed",
    "compile": "npm run clean && NODE_ENV=production webpack -p && npm run sass:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgmcdermott/bioinput.git"
  },
  "keywords": [
    "Bio",
    "input",
    "BD2K-LINCS",
    "BD2K",
    "LINCS",
    "DCIC",
    "autocomplete",
    "assay",
    "cell",
    "line",
    "cell-line"
  ],
  "author": "Michael McDermott <michael@mgmcdermott.com> (http://mgmcdermott.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgmcdermott/bioinput/issues"
  },
  "homepage": "https://github.com/mgmcdermott/bioinput#readme",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0-beta.3",
    "eslint-config-airbnb": "^4.0.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12"
  }
}
